CREATE TABLE CONSULTATION (
  id INT PRIMARY KEY NOT NULL,
  appointment_id INT REFERENCES appointments(id),
  patient_id INT REFERENCES patient(id),
  doctor_id INT REFERENCES doctors(id),
  in_time TIME,
  out_time TIME
);

