CREATE TABLE Users (
  id INT PRIMARY KEY NOT NULL,
  name varchar(30) NOT NULL,
  gender varchar(6) NOT NULL,
  dob Date NOT NULL,
  mobile_number varchar(10) NOT NULL,
  otp int NOT NULL,
  otp_expires_at TimeStamp,
  created_at TimeStamp NOT NULL,
  updated_at TimeStamp NOT NULL
);


